<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Wikipedia Collections admin{% endblock %}</title>
        {% block stylesheets %}
            <link rel="stylesheet" type="text/css" href="{{ asset('compiled/frontend.css') }}" />
        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body class="{% block bodyClass %}{% endblock %}">
        {% block body %}
            {% block content %}{% endblock %}
        {% endblock %}
        {% block javascripts %}
            <script src="{{ asset('vendor/jquery/dist/jquery.js') }}"></script>
            <script src="{{ asset('vendor/bootstrap/dist/js/bootstrap.js') }}"></script>
        {% endblock %}
    </body>
</html>
